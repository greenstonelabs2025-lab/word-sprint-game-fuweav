
{
  "name": "word-sprint-game-fuweav",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "preinstall": "npx only-allow npm",
    "postinstall": "node scripts/cleanup-package-managers.js",
    "start": "expo start -c",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild --clean",
    "dev": "EXPO_NO_TELEMETRY=1 expo start --tunnel",
    "build:web": "expo export -p web && npx workbox generateSW workbox-config.js",
    "build:android": "expo prebuild -p android --clean",
    "build:apk": "node scripts/build.js apk android",
    "build:aab": "node scripts/build.js aab android",
    "build:preview": "node scripts/build.js preview android",
    "build:play-console": "node scripts/build.js play-console android",
    "build:clean": "node scripts/build.js apk android --clean",
    "gradle:clean": "cd android && ./gradlew --stop && ./gradlew clean",
    "gradle:setup": "chmod +x android/gradlew && cd android && ./gradlew wrapper --gradle-version 8.0.2",
    "android:build": "cd android && ./gradlew :app:assembleRelease",
    "android:clean-build": "cd android && ./gradlew --stop && ./gradlew clean && ./gradlew :app:assembleRelease",
    "android:verify": "node scripts/verify-android-config.js",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "prebuild:clean": "rm -rf android ios && expo prebuild --clean",
    "fix-deps": "node scripts/fix-dependencies.js",
    "fix-deps:full": "node scripts/fix-dependencies.js --full",
    "verify-setup": "node scripts/verify-expo-setup.js",
    "check-metro": "node scripts/check-metro-imports.js",
    "clean-metro": "rm -rf .expo && rm -rf node_modules/.cache && rm -rf /tmp/metro-* && expo start -c",
    "cleanup-pm": "node scripts/cleanup-package-managers.js",
    "lint": "eslint ."
  },
