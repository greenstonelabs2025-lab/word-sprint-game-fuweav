
{
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "autoIncrement": true
    },
    "preview": {
      "distribution": "internal",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      }
    },
    "production-aab": {
      "extends": "production",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease"
      }
    },
    "play-console": {
      "extends": "production-aab",
      "android": {
        "buildType": "aab",
        "gradleCommand": ":app:bundleRelease",
        "cache": {
          "disabled": true
        }
      },
      "env": {
        "EXPO_NO_CACHE": "1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
